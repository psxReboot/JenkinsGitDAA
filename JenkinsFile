pipeline{
	   agent any
	            stages {
			    stage('One') {
                                        steps {
					                echo 'Hi, this is Dyubkin2'
					}
	 		  }
			    stage('Two') {
                                        steps {
					                input {
    								message "Введите значение:"
								parameters {
							        string(name: 'userInput', defaultValue: '', description: 'Значение от пользователя')
                                                                }
                                                        }
					}
	 		  }
	  
	            }
}